@page
@model Dashboard
@{
    ViewData["Title"] = "Dashboard";
}
<h1>@ViewData["Title"]</h1>
<div class="container-fluid">
    <div class="row">
        <div class="col-4">
            <partial name="_ProdottiCostosi" model="Model.ProdottiCostosi" />
        </div> 
        <div class="col-4">
            <partial name="_ProdottiRecenti" model="Model.ProdottiRecenti" />
        </div>
        <div class="col-4"> 
            <partial name="_ProdottiPerCategoria" model="Model.ProdottiPerCategoria" />
        </div>
    </div>
</div>